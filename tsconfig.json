{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "allowJs": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
